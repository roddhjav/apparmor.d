  unix (bind, listen) type=stream addr="@/tmp/dbus-*",

  unix (connect, receive, send, accept)
       type=stream
       peer=(addr="@/tmp/dbus-*"),

  owner @{run}/user/@{uid}/at-spi/ rw,
  owner @{run}/user/@{uid}/at-spi/bus{,_[0-9]*} rw,
