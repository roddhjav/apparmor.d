  include <abstractions/gtk>

  dbus receive bus=session
       interface=org.freedesktop.DBus.Introspectable
       member=Introspect 
       peer=(name=:*, label=gnome-shell),

  /var/cache/gio-@{int}.@{int}/gnome-mimeapps.list r,

