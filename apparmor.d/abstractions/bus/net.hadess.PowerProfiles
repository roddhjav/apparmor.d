  dbus send bus=system path=/net/hadess/PowerProfiles
       interface=org.freedesktop.DBus.Properties
       member=GetAll
       peer=(name=:*, label=power-profiles-daemon),

  include if exists <abstractions/bus/net.hadess.PowerProfiles.d>

# vim:syntax=apparmor
