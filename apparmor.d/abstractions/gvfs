# Allow access to GVFS files.

  abi <abi/4.0>,

  #aa:dbus talk bus=session name=org.gtk.vfs label="gvfsd{,-*}"

  owner @{run}/user/@{uid}/gvfsd/socket-@{rand8} rw,

  include if exists <abstractions/gvfs.d>

# vim:syntax=apparmor
