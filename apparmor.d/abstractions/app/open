# Full set of rules for child-open-* profiles.

  include <abstractions/desktop>

  @{open_path} mrix,

  @{sh_path} r,
  @{bin}/env rix,

  /dev/tty rw,

  include if exists <abstractions/app/open.d>

# vim:syntax=apparmor
