# Communication to the system TPM chip over /dev/tpm@{int} and kernel TPM
# resource manager /dev/tpmrm@{int}

  abi <abi/4.0>,

  /dev/tpm@{int} rw,
  /dev/tpmrm@{int} rw,

  include if exists <abstractions/tpm.d>

# vim:syntax=apparmor
