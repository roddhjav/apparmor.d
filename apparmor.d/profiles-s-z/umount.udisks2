abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/umount.udisks2
profile umount.udisks2 @{exec_path} flags=(complain) {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/umount.udisks2>
}

# vim:syntax=apparmor
