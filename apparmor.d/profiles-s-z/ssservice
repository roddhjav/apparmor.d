abi <abi/3.0>,

include <tunables/global>

@{exec_path} = /{,usr/}{,local/}bin/ssservice
profile ssservice @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/ssservice>
}
