abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/smbspool
profile smbspool @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  /etc/papersize r,

  include if exists <local/smbspool>
}

# vim:syntax=apparmor
