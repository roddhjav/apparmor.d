abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/whois
profile whois @{exec_path} {
  include <abstractions/base>
  include <abstractions/nameservice-strict>

  network inet dgram,
  network inet6 dgram,
  network inet stream,
  network inet6 stream,
  network netlink raw,

  @{exec_path} mr,

  /etc/whois.conf r,

  include if exists <local/whois.d>
}

# vim:syntax=apparmor
