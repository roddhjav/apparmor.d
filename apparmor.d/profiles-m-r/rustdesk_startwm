abi <abi/4.0>,

include <tunables/global>

@{exec_path} = /etc/rustdesk/startwm.sh
profile rustdesk_startwm @{exec_path} {
  include <abstractions/base-strict>

  @{exec_path} r,

  include if exists <local/rustdesk_startwm>
}

# vim:syntax=apparmor
