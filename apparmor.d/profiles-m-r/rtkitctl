abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{sbin}/rtkitctl
profile rtkitctl @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/rtkitctl>
}

# vim:syntax=apparmor
