abi <abi/4.0>,

include <tunables/global>

@{exec_path} = /usr/share/rustdesk/files/polkit
profile rustdesk_polkit @{exec_path} {
  include <abstractions/base-strict>

  @{exec_path} r,

  include if exists <local/rustdesk_polkit>
}

# vim:syntax=apparmor
