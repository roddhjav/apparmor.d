abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/rredtool
profile rredtool @{exec_path} flags=(complain) {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/rredtool>
}
