abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/crda
profile crda @{exec_path} {
  include <abstractions/base>

  # For "iw reg set PL"
  capability net_admin,

  @{exec_path} mr,

  include if exists <local/crda>
}

# vim:syntax=apparmor
