abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/cups/backend/cups-brf
profile cups-backend-brf @{exec_path} {
  include <abstractions/base>

  capability setuid,

  @{exec_path} mr,

  /etc/papersize r,

  include if exists <local/cups-backend-brf>
}

# vim:syntax=apparmor
