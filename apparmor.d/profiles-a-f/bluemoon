abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/bluemoon
profile bluemoon @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/bluemoon>
}

# vim:syntax=apparmor
