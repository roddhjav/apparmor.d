abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{sbin}/dmeventd
profile dmeventd @{exec_path} flags=(complain) {
  include <abstractions/base>

  @{exec_path} rm,

  include if exists <local/dmeventd>
}

# vim:syntax=apparmor
