abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/cert-sync
profile cert-sync @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  @{bin}/mono-sgen rPx,

  include if exists <local/cert-sync>
}