abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/cpuid
profile cpuid @{exec_path} {
  include <abstractions/base>

  capability mknod,

  @{exec_path} mr,

  /dev/cpu/@{int}/cpuid r,

  owner @{tmp}/cpuid* rw,

  include if exists <local/cpuid>
}

# vim:syntax=apparmor
