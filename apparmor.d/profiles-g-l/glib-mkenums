abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/glib-mkenums
profile glib-mkenums @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  @{bin}/ r,

  include if exists <local/glib-mkenums>
}

# vim:syntax=apparmor

