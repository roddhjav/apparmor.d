abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/keepassxc-cli
profile keepassxc-cli @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/keepassxc-cli>
}

