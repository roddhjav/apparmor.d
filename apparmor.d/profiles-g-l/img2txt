abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/img2txt
profile img2txt @{exec_path} {
  include <abstractions/base>
  include <abstractions/user-read-strict>

  @{exec_path} mr,

  include if exists <local/img2txt>
}
