abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/gping
profile gping @{exec_path} {
  include <abstractions/base>
  include <abstractions/consoles>

  @{exec_path} mr,

  @{bin}/ping rPx,

  include if exists <local/gping>
}

# vim:syntax=apparmor
