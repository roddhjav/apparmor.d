abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/ipcalc
profile ipcalc @{exec_path} {
  include <abstractions/base>
  include <abstractions/perl>

  @{exec_path} r,
  @{bin}/perl r,

  include if exists <local/ipcalc>
}

# vim:syntax=apparmor
