abi <abi/3.0>,

include <tunables/global>

@{exec_pathCHP} = /sbin/chpst @{bin}/chpst
profile chpst @{exec_pathCHP} {
  include <abstractions/base>
  include <abstractions/devices-usb>

  @{exec_pathCHP} 	mr,

  @{bin}/agetty		rPx,
  @{bin}/pause		rPx,

}
