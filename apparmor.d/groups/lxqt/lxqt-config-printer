abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/lxqt-config-printer
profile lxqt-config-printer @{exec_path} {
  include <abstractions/base>
  include <abstractions/lxqt>

  @{exec_pathlx15} mr,

  owner /tmp/@{int} r,

  /dev/tty rw,

  include if exists <local/lxqt-config-printer>
}

# vim:syntax=apparmor
