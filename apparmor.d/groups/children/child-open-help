abi <abi/4.0>,

include <tunables/global>

profile child-open-help {
  include <abstractions/base>
  include <abstractions/app/open>

  @{browsers_path}              rPx,
  @{help_path}                  rPx,

  include if exists <usr/child-open-help.d>
  include if exists <local/child-open-help>
}

