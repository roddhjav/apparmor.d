abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/grub-set-default
profile grub-set-default @{exec_path} {
  include <abstractions/base>
  include <abstractions/consoles>

  @{exec_path} mr,

  include if exists <local/grub-set-default>
}

