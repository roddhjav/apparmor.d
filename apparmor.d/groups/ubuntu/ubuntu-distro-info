abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/ubuntu-distro-info
profile ubuntu-distro-info @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  /usr/share/distro-info/*.csv r,

  include if exists <local/ubuntu-distro-info>
}