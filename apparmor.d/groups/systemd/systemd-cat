abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/systemd-cat
profile systemd-cat @{exec_path} {
  include <abstractions/base>

  capability net_admin,

  @{exec_path} mr,

  @{bin}/cat rix,

  include if exists <local/systemd-cat>
}
