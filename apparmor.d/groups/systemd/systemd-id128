abi <abi/3.0>,

include <tunables/global>

@{exec_path} = /{usr/,}bin/systemd-id128
profile systemd-id128 @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  /etc/machine-id r,

  @{PROC}/sys/kernel/random/boot_id r,

  include if exists <local/systemd-id128>
}
