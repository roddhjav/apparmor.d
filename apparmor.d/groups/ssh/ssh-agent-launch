abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/openssh/agent-launch
profile ssh-agent-launch @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  @{bin}/{,z,ba,da}sh  rix,

  include if exists <local/ssh-agent-launch>
}