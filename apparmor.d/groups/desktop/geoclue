abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{libexec}/geoclue
profile geoclue @{exec_path} {
  include <abstractions/base>

  network netlink raw,

  @{exec_path} mr,

  /etc/geoclue/{,**} r,

  @{PROC}/@{pids}/cgroup r,

  include if exists <local/geoclue>
}