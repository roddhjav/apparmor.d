abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/msgcollector/striphtml
profile msgcollector-striphtml @{exec_path} {
  include <abstractions/base>
  include <abstractions/python>

  @{exec_path} mr,

  @{lib}/msgcollector/ r,

  include if exists <local/msgcollector-striphtml>
}