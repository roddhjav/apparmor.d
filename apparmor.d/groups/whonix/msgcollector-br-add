abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/msgcollector/br_add
profile msgcollector-br-add @{exec_path} {
  include <abstractions/base>
  include <abstractions/python>

  @{exec_path} mr,

  include if exists <local/msgcollector-br-add>
}

