abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{bin}/avahi-set-host-name
profile avahi-set-host-name @{exec_path} {
  include <abstractions/base>
  include <abstractions/consoles>

  @{exec_path} mr,

  include if exists <local/avahi-set-host-name>
}

