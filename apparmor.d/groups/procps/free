abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/free
profile free @{exec_path} flags=(attach_disconnected) {
  include <abstractions/base>
  include <abstractions/consoles>

  @{exec_path} mr,

  include if exists <local/free>
}

# vim:syntax=apparmor
