abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{bin}/gnome-font-viewer
profile gnome-font-viewer @{exec_path} {
  include <abstractions/base>
  include <abstractions/common/gnome>

  @{exec_path} mr,

  include if exists <local/gnome-font-viewer>
}

# vim:syntax=apparmor
