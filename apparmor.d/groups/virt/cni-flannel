abi <abi/4.0>,

include <tunables/global>

@{exec_path} = @{lib}/cni/flannel /opt/cni/bin/flannel
profile cni-flannel @{exec_path} flags=(complain,attach_disconnected) {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/cni-flannel>
}

# vim:syntax=apparmor
