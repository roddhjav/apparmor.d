abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/cockpit/cockpit-askpass
profile cockpit-askpass @{exec_path} {
  include <abstractions/base>
  include <abstractions/python>

  @{exec_path} mr,

  include if exists <local/cockpit-askpass>
}