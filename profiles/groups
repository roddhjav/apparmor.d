abi <abi/3.0>,

include <tunables/global>

@{exec_path} = /{usr/,}bin/groups
profile groups @{exec_path} {
  include <abstractions/base>
  include <abstractions/nameservice-strict>

  @{exec_path} mr,

  include if exists <local/groups>
}
